{
  "name": "react-native-google-calendar-api",
  "version": "0.0.1",
  "description": "React Native Module for Google Calendar API interaction",
  "main": "ApiCalendar.js",
  "scripts": {
    "build": "eslint . --ext .ts,.tsx && tsup src/ApiCalendar.ts --dts",
    "dev": "tsup src/ApiCalendar.ts --dts --watch",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "bundleDependencies": false,
  "dependencies": {
    "@types/node": "^9.6.18"
  },
  "devDependencies": {
    "react": "18.3.1",
    "react-native": "0.75.3",
    "@types/gapi": "^0.0.44",
    "@types/gapi.calendar": "^3.0.6",
    "@types/google.accounts": "^0.0.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "jest": "^29.4.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.2",
    "tsup": "^6.0.1"
  },
  "keywords": [
    "react",
    "native",
    "google",
    "calendar",
    "api"
  ],
  "files": [
    "dist"
  ]
}
